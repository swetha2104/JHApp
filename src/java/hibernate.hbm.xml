<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.mangium.Car" table="cars">
  <id name="carid" type="java.lang.String">
      <column name="carid" />
      <generator class="assigned"/>
      
  </id>
  <property name="carname" type="java.lang.String">
      <column name="carname" />
  </property>
    <property name="carcost" type="java.lang.String">
      <column name="carcost" />
  </property>
    
  </class>
</hibernate-mapping>
